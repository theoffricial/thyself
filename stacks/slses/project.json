{
  "name": "slses",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "stacks/slses/src",
  "targets": {
    "build": {
      "executor": "@thyself/nx-serverless:package",
      "options": {
        "cwd": "stacks/slses",
        "stage": "dev"
      }
    },
    "start": {
      "executor": "@thyself/nx-serverless:offline-start",
      "options": {
        "cwd": "stacks/slses",
        "stage": "dev"
      }
    },
    "deploy": {
      "executor": "@thyself/nx-serverless:deploy",
      "options": {
        "cwd": "stacks/slses",
        "stage": "dev"
      }
    },
    "remove": {
      "executor": "@thyself/nx-serverless:remove",
      "options": {
        "cwd": "stacks/slses",
        "stage": "dev"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["stacks/slses/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "stacks/slses/jest.config.ts"
      }
    }
  }
}
