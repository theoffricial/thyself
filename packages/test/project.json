{
  "name": "test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/test/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/test",
        "main": "packages/test/src/index.ts",
        "tsConfig": "packages/test/tsconfig.lib.json",
        "assets": ["packages/test/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/test"
      }
    },
    "echo": {
      "executor": "@finance-hub/nx-serverless:echo",
      "options": {
        "textToEcho": "Hello World"
      }
    },
    "print": {
      "executor": "@finance-hub/nx-serverless:print",
      "options": {
        "textToEcho": "Hello World"
      }
    }
  },
  "tags": []
}
