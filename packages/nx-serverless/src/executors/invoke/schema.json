{
  "version": 2,
  "title": "Invoke executor",
  "description": "",
  "type": "object",
  "properties": {
    "cwd": {
      "type": "string",
      "description": "Current working directory"
    },
    "no-colors": { 
        "type": "boolean",
        "description": "Turn off colors in CLI output",
        "default": false
    },
    "serverless-file-ext": { 
      "type": "string", 
      "enum": ["ts", "js", "yml", "json"] ,
      "default": "ts",
      "description": "Serverless configuration file name, Default: serverless.ts"
    },
    "region": { 
        "type": "string",
        "description": "region where the service should be deployed"
    },
    "aws-profile": { 
        "type": "string",
        "description": "AWS profile to use for deployment, Default: default"
    },
    "app": { 
        "type": "string",
        "description": "Serverless app name"
    },
    "org": { 
        "type": "string",
        "description": "Serverless organization name"
    },
    "use-local-credentials": { 
        "type": "boolean",
        "description": "Use local credentials instead of AWS_PROFILE"
    },
    "config": { 
        "type": "string",
        "description": "Path to serverless configuration file"
    },
    "stage": { 
        "type": "string",
        "description": "Stage to deploy to"
    },
    "param": { 
        "type": "string",
        "description": "Parameter to override in serverless configuration file"
    },
    "help": { 
        "type": "boolean",
        "description": "Shows this message and quits"
    },
    "version": { 
        "type": "boolean",
        "description": "Shows version information and quits"
    },
    "verbose": { 
        "type": "boolean",
        "description": "Shows verbose output"
    },
    "debug": { 
        "type": "boolean",
        "description": "Shows debug output"
    },
    "function": {
      "type": "string",
      "description": "The function name"
    },
    "qualifier": {
      "type": "string",
      "description": "Version number or alias to invoke"
    },
    "path": {
      "type": "string",
      "description": "Path to JSON or YAML file holding input data"
    },
    "type": {
      "type": "string",
      "description": "Type of invocation"
    },
    "log": {
      "type": "boolean",
      "description": "Trigger logging data output"
    },
    "data": {
      "type": "object",
      "description": "Input data"
    },
    "raw": {
      "type": "boolean",
      "description": "Flag to pass input data as a raw string"
    },
    "context": {
      "type": "object",
      "description": "Context of the service"
    },
    "contextPath": {
      "type": "string",
      "description": "Path to JSON or YAML file holding context data"
    }
  },
  "required": []
}