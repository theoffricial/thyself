    "noColors": { "type": "boolean" },
    "serverlessConfigurationFileName": { 
      "type": "string", 
      "enum": ["serverless.ts", "serverless.js", "serverless.yml", "serverless.json"] ,
      "default": "serverless.ts"
    },
    "path": { "type": "string" },
    "transform": { "type": "string" },
    
    "format": { "type": "string", "enum": ["json", "yaml", "text"] },
    "region": { "type": "string" },
    "awsProfile": { "type": "string" },
    "app": { "type": "string" },
    "org": { "type": "string" },
    "useLocalCredentials": { "type": "boolean" },
    "config": { "type": "string" },
    "stage": { "type": "string" },
    "param": { "type": "string" },
    "help": { "type": "boolean" },
    "version": { "type": "boolean" },
    "verbose": { "type": "boolean" },
    "debug": { "type": "boolean" }
  },
  "required": []
}
