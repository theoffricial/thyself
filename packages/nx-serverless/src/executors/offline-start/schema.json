    "app": { "type": "string" },
    "org": { "type": "string" },
    "useLocalCredentials": { "type": "boolean" },
    "config": { "type": "string" },
    "stage": { "type": "string" },
    "param": { "type": "string" },
    "help": { "type": "boolean" },
    "version": { "type": "boolean" },
    "verbose": { "type": "boolean" },
    "debug": { "type": "boolean" }
