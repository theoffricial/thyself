{
  "name": "my-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "stacks/my-service/src",
  "targets": {
    "start": {
      "executor": "@finance-hub/nx-serverless:offline-start",
      "options": {
        "cwd": "stacks/my-service"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "stacks/my-service",
        "color": true,
        "command": "serverless package"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "stacks/my-service",
        "color": true,
        "command": "serverless offline start"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "stacks/my-service",
        "color": true,
        "command": "serverless deploy"
      }
    },
    "remove": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "stacks/my-service",
        "color": true,
        "command": "serverless remove"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["stacks/my-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "stacks/my-service/jest.config.ts"
      }
    }
  }
}
